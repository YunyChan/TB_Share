{
  "manifest_version": 2,
  "name": "淘宝分享",
  "version": "1.1.0",
  "description": "淘宝链接去跟踪一键复制工具",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "clipboardWrite"
  ],
  "page_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "淘宝链接一键复制",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  }
}
